{
  "name": "nuxt-workshop",
  "version": "1.0.0",
  "description": "2019/07/20 leafage workshop",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": "https://github.com/leafage-gk/nuxt-workshop.git",
  "author": "kento <kento@leafage.co.jp>",
  "license": "MIT",
  "dependencies": {
    "nuxt": "^2.8.1",
    "vue": "^2.6.10",
    "vuetify": "^1.5.16"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.1",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.1",
    "lerna": "^3.16.1",
    "lint-staged": "^9.2.0",
    "rimraf": "^2.6.3",
    "prettier": "1.18.2",
    "typescript": "^3.5.3"
  },
  "private": true
}
